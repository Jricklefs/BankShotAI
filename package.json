{
  "name": "bankshotai-app",
  "version": "1.0.0",
  "description": "BankShotAI - AR pool bank shot calculator",
  "main": "src/index.html",
  "scripts": {
    "start": "npx serve src -l 8100",
    "cap:init": "npx cap init BankShotAI com.bankshotai.app --web-dir src",
    "cap:add:ios": "npx cap add ios",
    "cap:add:android": "npx cap add android",
    "cap:sync": "npx cap sync",
    "cap:open:ios": "npx cap open ios",
    "cap:open:android": "npx cap open android"
  },
  "dependencies": {
    "@capacitor/core": "^6.0.0",
    "@capacitor/camera": "^6.0.0",
    "@capacitor/haptics": "^6.0.0"
  },
  "devDependencies": {
    "@capacitor/cli": "^6.0.0",
    "serve": "^14.2.0"
  }
}
